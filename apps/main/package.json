    "expo-print": "~55.0.8",
    "expo-router": "~55.0.3",
    "expo-splash-screen": "~55.0.10",
    "expo-sqlite": "~55.0.10",
    "expo-system-ui": "~55.0.9",
    "expo-web-browser": "~55.0.9",
    "js-base64": "^3.7.7",
    "postcss": "^8.5.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
