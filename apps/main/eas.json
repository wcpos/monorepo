{
  "cli": {
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "monorepo": {
      "pnpm": "10.10.0",
      "cache": {
        "key": "turbo",
        "paths": [
          "../../.turbo/cache"
        ]
      },
      "env": {
        "EXPO_USE_FAST_RESOLVER": "true"
      }
    },
    "development": {
      "extends": "monorepo",
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "monorepo",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "adhoc": {
      "extends": "monorepo",
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "paul@kilbot.com.au",
        "ascAppId": "6742204061",
        "appleTeamId": "G7L8G4KJ7A"
      }
    }
  }
}